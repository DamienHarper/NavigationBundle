{
  "name": "damienharper/navigation-bundle",
  "type": "symfony-bundle",
  "description": "Provides routing, distance matrix features etc from different providers",
  "keywords": ["routing", "distance matrix"],
  "license": "MIT",
  "authors": [
    {
      "name": "Damien Harper",
      "email": "damien.harper@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "^3.3 || ^4.0",
    "symfony/options-resolver": "^3.3 || ^4.0",
    "guzzlehttp/guzzle": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "DH\\NavigationBundle\\": "src/NavigationBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DH\\NavigationBundle\\Tests\\": "tests/NavigationBundle"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0",
    "matthiasnoback/symfony-dependency-injection-test": "^3.1 || ^4.0",
    "symfony/var-dumper": "^4.2",
    "phpstan/phpstan": "^0.11.2",
    "phpstan/phpstan-doctrine": "^0.11.1",
    "phpstan/phpstan-symfony": "^0.11.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
